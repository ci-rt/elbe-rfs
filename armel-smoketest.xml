<ns0:RootFileSystem xmlns:ns0="https://www.linutronix.de/projects/Elbe" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" created="2009-05-20T08:50:56" revision="6" xsi:schemaLocation="https://www.linutronix.de/projects/Elbe dbsfed.xsd">
  <project>
    <name>armel-smoketest</name>
    <version>1.0</version>
    <description>armel debian jessie smoketest mini RFS</description>
    <buildtype>armel</buildtype>
    <mirror>
      <primary_host>httpredir.debian.org</primary_host>
      <primary_path>/debian</primary_path>
      <primary_proto>http</primary_proto>
    </mirror>
    <suite>jessie</suite>
  </project>
  <target>
    <hostname>smoke-armel</hostname>
    <domain>ci-rt.example.com</domain>
    <passwd>foo</passwd>
    <console>ttyS0,115200</console>
    <package>
      <tar>
	<name>ci-rt-rfs_armel.tar.gz</name>
      </tar>
    </package>
    <fstab>
      <bylabel>
	<label>rfs</label>
	<mountpoint>/</mountpoint>
	<fs>
	  <type>ext4</type>
	  <tune2fs>-i 0</tune2fs>
	</fs>
      </bylabel>
    </fstab>
    <finetuning>
      <rm>var/cache/apt/archives/*.deb</rm>
      <adduser passwd="jenkins" shell="/bin/bash">jenkins</adduser>
      <command>chown -R jenkins:jenkins /boot/jenkins</command>
      <command>chown -R jenkins:jenkins /home/jenkins</command>
      <command>chown jenkins:jenkins /lib/modules</command>
    </finetuning>
    <pkg-list>
      <pkg>bash</pkg>
      <pkg>busybox</pkg>
      <pkg>default-jre-headless</pkg>
      <pkg>kexec-tools</pkg>
      <pkg>ntp</pkg>
      <pkg>ntpdate</pkg>
      <pkg>rt-tests</pkg>
      <pkg>sudo</pkg>
      <pkg>openssh-server</pkg>
    </pkg-list>
  </target>
